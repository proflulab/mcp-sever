      "args": [
        "--from",
        "git+https://github.com/proflulab/DocumentAssistant.git",
        "python",
        "-m",
        "document_assistant.server"
      ],
      "env": {
        "MCP_TRANSPORT": "stdio"
      }
    }
  }
}